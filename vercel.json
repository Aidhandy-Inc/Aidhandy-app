{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/contact", "destination": "/contact.html" },
    { "source": "/privacy-policy", "destination": "/privacy-policy.html" },
    { "source": "/terms", "destination": "/terms.html" },
    { "source": "/cookie-policy", "destination": "/cookie-policy.html" },
    { "source": "/messaging-terms", "destination": "/messaging-terms.html" },
    { "source": "/thankyou", "destination": "/thankyou.html" },

    { "source": "/app/:path*", "destination": "/nextapp/app/:path*" },
    { "source": "/dashboard/:path*", "destination": "/nextapp/dashboard/:path*" },
    { "source": "/auth/:path*", "destination": "/nextapp/auth/:path*" },

    { "source": "/api/:path*", "destination": "/nextapp/api/:path*" }
  ]
}

